article.card#card-
  header.card__header
    h3.card__header__pin.cursor#card-pin D0
    h3.card__header__pin.cursor#card-pin ESP8266
    i.card__header__menu.icon-menu#card-menu
  .card__body
    h1.card__body__icon.pointer.icons-size.icns-red#card-icon 33.54V
    h2.card__body__text.pointer#card-text Voltimetro
  footer.card__footer
    input(type="url" placeholder="topic" title="topic").card__footer__url#card-url
    .card__footer__btn#btn-on-off
      label.switch
        input(type='checkbox' checked='')
        span.slider.round
